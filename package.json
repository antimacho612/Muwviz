{
  "name": "muwviz",
  "version": "0.10.1",
  "private": true,
  "author": "antimacho612 <antimacho612@gmail.com>",
  "description": "A desktop music player with visualizers.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antimacho612/Muwviz.git"
  },
  "main": "./out/main/index.js",
  "scripts": {
    "format": "prettier src --write",
    "lint": "eslint src --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "dev:watch": "electron-vite dev --watch",
    "build": "npm run typecheck && electron-vite build",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "npm run build && electron-builder --mac --config",
    "build:linux": "npm run build && electron-builder --linux --config",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^2.0.0"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^1.0.1",
    "@electron-toolkit/eslint-config-ts": "^1.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@imengyu/vue3-context-menu": "^1.3.3",
    "@rushstack/eslint-patch": "^1.3.3",
    "@types/eslint": "^8.44.6",
    "@types/node": "^18.17.5",
    "@vitejs/plugin-vue": "^4.3.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vueuse/core": "^10.6.0",
    "animate.css": "^4.1.1",
    "audiomotion-analyzer": "^4.3.0",
    "d3": "^7.8.5",
    "electron": "^28.1.1",
    "electron-builder": "^24.6.3",
    "electron-log": "^5.0.3",
    "electron-typescript-ipc": "^3.0.0",
    "electron-updater": "^6.1.7",
    "electron-vite": "^1.0.27",
    "eslint": "^8.52.0",
    "eslint-plugin-vue": "^9.17.0",
    "fast-deep-equal": "^3.1.3",
    "genius-lyrics-api": "^3.2.0",
    "jimp": "^0.22.10",
    "js-xxhash": "^3.0.0",
    "modern-css-reset": "^1.4.0",
    "music-metadata": "^7.14.0",
    "pinia": "^2.1.7",
    "prettier": "^3.0.2",
    "primeflex": "^3.3.1",
    "sass": "^1.69.5",
    "toad-scheduler": "^3.0.0",
    "typescript": "^5.2.2",
    "v-wave": "^2.0.0",
    "vite": "^4.4.9",
    "vite-svg-loader": "^5.1.0",
    "vue": "^3.4.4",
    "vue-history-state": "^0.14.0",
    "vue-router": "^4.2.5",
    "vue-toastification": "^2.0.0-rc.5",
    "vue-tsc": "^1.8.8",
    "vue-virtual-scroller": "^2.0.0-beta.8",
    "waveform-data": "^4.4.0"
  }
}
